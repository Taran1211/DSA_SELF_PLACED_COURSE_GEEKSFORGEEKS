class GfG
{
    //Function to locate the occurrence of the string x in the string s.
    int strstr(String s, String x)
    {
       int m=x.length();
    int n=s.length();
    for(int i=0;i<=n-m;i++)
    {
    int j;
    for( j=0;j<m;j++)
    {
    if(s.charAt(i+j)!=x.charAt(j))
    break;
    }
    if(j==m)
    return i;
    }
    return -1;
    }
    
}
    
